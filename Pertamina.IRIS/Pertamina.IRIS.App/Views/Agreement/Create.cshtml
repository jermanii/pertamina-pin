@using Pertamina.IRIS.Models.Base
@inject GlobalVariableBaseModel globalParam
@model Pertamina.IRIS.Models.ViewModels.AgreementViewModel

@{
    ViewData["Title"] = "Agreement";
    Layout = "~/Views/Shared/_Layout.cshtml";

    var controllerName = this.ViewContext.RouteData.Values["controller"].ToString();
}

<div class="app-main flex-column flex-row-fluid">
    <div class="d-flex flex-column flex-column-fluid">
        <div class="app-toolbar py-3 py-lg-6">
            <div class="app-container container-xxl d-flex flex-stack">
                <div class="page-title d-flex flex-column justify-content-center flex-wrap me-3">
                    <h1 class="page-heading d-flex text-dark fw-bold fs-3 flex-column justify-content-center my-0">ADD AGREEMENT </h1>
                </div>
            </div>
        </div>
        <div class="app-content flex-column-fluid" style="padding-bottom: 1px;">
            <div class="app-container container-xxl">
                <div class="card card-custom">
                    <div class="card-header">
                        <div class="row">
                            <h3 class="card-title">
                                SELECT HOW TO CREATE AN AGREEMENT
                            </h3>
                        </div>
                    </div>
                    <div class="card-body">
                        <div class="form-group row">
                            <div class="col-lg-6">
                                <div class="fv-row mb-10">
                                    <label for="sumberPengisianInput" class="required form-label">Select Data Fill Source</label>
                                    @Html.DropDownListFor(x => x.SelectedFormType,
                                             new MultiSelectList(Enumerable.Empty<SelectListItem>()),
                                             new
                                             {
                                                 @id = "sumberPengisianInput",
                                                 @class = " form-control form-select form-select-solid form-select-lg",
                                                 aria_label = "sumberPengisianInput",
                                                 data_control = "select2",
                                                 data_placeholder = "Select Data Fill Source",
                                                 data_allow_clear = "true"
                                             })
                                </div>
                            </div>
                            <div class="col-lg-6">
                                <div id="ddlJudulOpportunity" class="fv-row mb-10">
                                    <label for="judulInput" class="required form-label">Select Title</label>
                                    @Html.DropDownListFor(x => x.JudulId,
                                             new MultiSelectList(Enumerable.Empty<SelectListItem>()),
                                             new
                                             {
                                                 @id = "judulInput",
                                                 @class = "form-select form-select-solid form-select-lg fw-semibold",
                                                 aria_label = "Pilih Judul",
                                                 data_control = "select2",
                                                 data_placeholder = "Select Title ",
                                                 data_allow_clear = "true",
                                                 maxlength = 1000,
                                             })
                                </div>
                                <div id="ddlJudulInitiative" class="fv-row mb-10">
                                    <label for="judulInputInitiative" class="required form-label">Select Title</label>
                                    @Html.DropDownListFor(x => x.JudulId,
                                             new MultiSelectList(Enumerable.Empty<SelectListItem>()),
                                             new
                                             {
                                                 @id = "judulInputInitiative",
                                                 @class = "form-select form-select-solid form-select-lg fw-semibold",
                                                 aria_label = "Pilih Judul",
                                                 data_control = "select2",
                                                 data_placeholder = "Select Title ",
                                                 data_allow_clear = "true",
                                                 maxlength = 1000,
                                             })
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row mb-2" style="padding-right: 10px;">
                    <div id="buttonCancel" class="col-lg-12 d-flex justify-content-end pb-0 px-0" style="padding-top: 6px;">
                        <button id="btnCancel" type="submit" class="btn font-weight-bold btn-secondary" onclick="location.href='@Url.Action("Index","Agreement")'">
                            Back
                        </button>
                    </div>
                </div>
            </div>
        </div>
        <div id="CreateBlankAgreement" class="app-container container-xxl">
            <form id="AddBlank" class="form" action="#" autocomplete="off">
                <div class="row">
                    <div class=" row">
                        <div class="col-lg-6">
                            <div class="flex-row-fluid">
                                <div class="card card-custom">
                                    <div class="card-header">
                                        <div class="row">
                                            <h3 class="card-title">
                                                HEADER
                                            </h3>
                                        </div>
                                    </div>
                                    <div class="card-body">
                                        <div class="fv-row mb-10">
                                            <label for="judulPerjanjianInput" class="required form-label">Agreement Title</label>
                                            <span class="ms-1" data-bs-toggle="tooltip" title="This column refers to the title of the agreement">
                                                <i class="ki-duotone ki-information-5 text-gray-500 fs-6">
                                                    <span class="path1"></span>
                                                    <span class="path2"></span>
                                                    <span class="path3"></span>
                                                </i>
                                            </span>
                                            @Html.TextBoxFor(m => m.JudulPerjanjian, new
                                                {
                                                    @id = "judulPerjanjianInput",
                                                    @class = "form-control form-control-solid",
                                                    placeholder = "Enter Agreement Title"

                                                })
                                        </div>
                                        <div class="fv-row mb-10">
                                            <label for="jenisPerjanjianInput" class="required form-label">Agreement Type</label>
                                            <span class="ms-1" data-bs-toggle="tooltip" title="This column refers to the type of agreement">
                                                <i class="ki-duotone ki-information-5 text-gray-500 fs-6">
                                                    <span class="path1"></span>
                                                    <span class="path2"></span>
                                                    <span class="path3"></span>
                                                </i>
                                            </span>
                                            @Html.DropDownListFor(x => x.JenisPerjanjianId,
                                                     new SelectList(Enumerable.Empty<SelectListItem>()),
                                                     new
                                                     {
                                                         @id = "jenisPerjanjianInput",
                                                         @class = " form-control form-select form-select-solid form-select-lg",
                                                         aria_label = "jenisPerjanjianInput",
                                                         data_control = "select2",
                                                         data_placeholder = "Enter Agreement Type",
                                                         data_allow_clear = "true"
                                                     })
                                        </div>
                                        <div class="fv-row mb-10">
                                            <label for="hshEntitasPertaminaInput" class="required form-label">Holding/Sub Holding - Pertamina Entity</label>
                                            <span class="ms-1" data-bs-toggle="tooltip" title="This column refers to the holding of the agreement">
                                                <i class="ki-duotone ki-information-5 text-gray-500 fs-6">
                                                    <span class="path1"></span>
                                                    <span class="path2"></span>
                                                    <span class="path3"></span>
                                                </i>
                                            </span>
                                            @Html.DropDownListFor(x => x.EntitasPertaminaEvent,
                                                     new MultiSelectList(Enumerable.Empty<SelectListItem>()),
                                                     new
                                                     {
                                                         @id = "hshEntitasPertaminaInput",
                                                         @class = "form-select form-select-solid form-select-lg fw-semibold",
                                                         aria_label = "hshEntitasPertaminaInput",
                                                         data_control = "select2",
                                                         data_placeholder = "Choose Holding/Sub Holding - Pertamina Entity",
                                                         data_allow_clear = "true"
                                                     })
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="app-content flex-column-fluid">
                                <div class="card card-custom">
                                    <div class="card-header">
                                        <div class="row">
                                            <h3 class="card-title">
                                                PIC INFORMATION
                                            </h3>
                                        </div>
                                    </div>
                                    <div class="card-body">
                                        <div class="form-group row">
                                            <div class="fv-row col-lg-5">
                                                @if (@Model.AgreementPicFungsi != null)
                                                {
                                                    <input type="hidden" value="@Model.AgreementPicFungsi.PicFungsiId" id="hdnPicInput"></input>
                                                }
                                                <label for="picInput" class="required form-label">Function - PIC</label>
                                                <span class="ms-1" data-bs-toggle="tooltip" title="This column refers to the name of the project PIC">
                                                    <i class="ki-duotone ki-information-5 text-gray-500 fs-6">
                                                        <span class="path1"></span>
                                                        <span class="path2"></span>
                                                        <span class="path3"></span>
                                                    </i>
                                                </span>
                                                @Html.DropDownListFor(x => x.AgreementPicFungsi.PicFungsiId,
                                                         new MultiSelectList(Enumerable.Empty<SelectListItem>()),
                                                         new
                                                         {
                                                             @id = "picInput",
                                                             @name = "PicFungsiId",
                                                             @class = "form-select form-select-solid form-select-md fw-semibold",
                                                             aria_label = "Choose PIC Lead - Function Name - Company",
                                                             data_placeholder = "Choose PIC - Function Name - Company",
                                                             data_control = "select2",
                                                         })
                                            </div>
                                            <div class="fv-row col-lg-5">
                                                <label for="picLevelInput" class="required form-label">PIC Level</label>
                                                <span class="ms-1" data-bs-toggle="tooltip" title="This column refers to the level of the selected PIC">
                                                    <i class="ki-duotone ki-information-5 text-gray-500 fs-6">
                                                        <span class="path1"></span>
                                                        <span class="path2"></span>
                                                        <span class="path3"></span>
                                                    </i>
                                                </span>
                                                @Html.HiddenFor(x => x.PicLevelId, new { @id = "picLevelId" })
                                                @Html.TextBoxFor(x => x.PicLevelName,
                                                         new
                                                         {
                                                             @id = "picLevelInput",
                                                             @name = "PicLevelId",
                                                             @class = "form-control form-control-solid",
                                                             maxlength = 6000,
                                                             placeholder = "Choose PIC Level Lead",
                                                             @readonly = "readonly",
                                                         })
                                            </div>
                                        </div>
                                        <div class="form-group mt-5">
                                            <div id="addRepeaterPic">
                                                @Html.HiddenFor(x => x.PicLevelMemberId, new { @id = "picLevelMemberId" })
                                                <div class="form-group">
                                                    <div data-repeater-list="PicName">
                                                        <div data-repeater-item class="pic mb-2">
                                                            <div class="form-group row">
                                                                <div class="fv-row col-lg-5 picFungsi">
                                                                    @* <label for="picFungsiMemberInput" class="required form-label">Partner(s)</label> *@
                                                                    @Html.DropDownListFor(x => x.AgreementPicFungsi.PicFungsiId,
                                                                             new MultiSelectList(Enumerable.Empty<SelectListItem>()),
                                                                             new
                                                                             {
                                                                                 @id = "picFungsiMemberInput",
                                                                                 @name = "PicMemberFungsiId",
                                                                                 @class = "form-select form-select-solid form-select-md fw-semibold fungsiPicMember",
                                                                                 aria_label = "Choose PIC Member- Function Name - Company",
                                                                                 data_placeholder = "Choose PIC - Function Name - Company",
                                                                             })
                                                                </div>
                                                                <div class="fv-row col-lg-5">
                                                                    @Html.TextBoxFor(x => x.PicLevelNameMember,
                                                                             new
                                                                             {
                                                                                 @id = "picLevelMemberInput",
                                                                                 @name = "PicMemberLevelId",
                                                                                 @class = "form-control form-control-solid levelPicMember",
                                                                                 maxlength = 6000,
                                                                                 placeholder = "Choose PIC Level Member",
                                                                                 @readonly = "readonly",
                                                                                 data_dropdown_parent = "#CreateFormExistingFootprints"
                                                                             })
                                                                </div>
                                                                <div class="col-lg-2 d-flex align-items-end">
                                                                    <button class="border border-secondary btn btn-icon btn-flex btn-danger" data-repeater-delete type="button">
                                                                        <i class="la la-close"></i>
                                                                    </button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="form-group">
                                                    <div class="form-group mt-5">
                                                        <a href="javascript:;" data-repeater-create class="btn font-weight-bold btn-warning" id="addPICButton">
                                                            <i class="ki-duotone ki-plus fs-3"></i>
                                                            Add PIC
                                                        </a>
                                                    </div>
                                                    <div class="col"></div>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <div class="flex-column-fluid">
                                <div class="card card-custom">
                                    <div class="card-header">
                                        <div class="row">
                                            <h3 class="card-title">
                                                PROJECT VALUE
                                            </h3>
                                        </div>
                                    </div>
                                    <div class="card-body">
                                        <div class="fv-row mb-10">
                                            <label for="transactionValue" class="form-label">Transaction Value</label>
                                            <span class="ms-1" data-bs-toggle="tooltip" title="This column refers to the transaction value of the agreement ">
                                                <i class="ki-duotone ki-information-5 text-gray-500 fs-6">
                                                    <span class="path1"></span>
                                                    <span class="path2"></span>
                                                    <span class="path3"></span>
                                                </i>
                                            </span>
                                            @Html.TextBoxFor(m => m.PotentialRevenuePerYearToString, new
                                                {
                                                    @id = "transactionValueInput",
                                                    @class = "form-control form-control-solid",
                                                    placeholder = "Enter Revenue",
                                                    maxlength = 15,
                                                    @type = "text",
                                                })
                                        </div>
                                        <div class="fv-row mb-10">
                                            <label for="projectCost" class="form-label">Project Cost</label>
                                            <span class="ms-1" data-bs-toggle="tooltip" title="This column refers to the cost of the agreement">
                                                <i class="ki-duotone ki-information-5 text-gray-500 fs-6">
                                                    <span class="path1"></span>
                                                    <span class="path2"></span>
                                                    <span class="path3"></span>
                                                </i>
                                            </span>
                                            @Html.TextBoxFor(m => m.CapexToString, new
                                                {
                                                    @id = "projectCostInput",
                                                    @class = "form-control form-control-solid",
                                                    placeholder = "Enter Project Cost",
                                                    maxlength = 15,
                                                    @type = "text",
                                                })
                                        </div>
                                        <div class="fv-row mb-10">
                                            <label for="nilaiProyekInput" class="form-label">Valuation (USD)</label>
                                            <span class="ms-1" data-bs-toggle="tooltip" title="This column refers to the estimation of the agreement">
                                                <i class="ki-duotone ki-information-5 text-gray-500 fs-6">
                                                    <span class="path1"></span>
                                                    <span class="path2"></span>
                                                    <span class="path3"></span>
                                                </i>
                                            </span>
                                            @Html.TextBoxFor(m => m.ValuationToString, new
                                                {
                                                    @id = "nilaiProyekInput",
                                                    @class = "form-control form-control-solid",
                                                    placeholder = "Enter Valuation",
                                                    @type = "text",
                                                    maxlength = 15
                                                })
                                        </div>

                                    </div>
                                </div>
                            </div>
                            <div class="app-content flex-column-fluid">
                                <div class="card card-custom">
                                    <div class="card-header">
                                        <div class="row">
                                            <h3 class="card-title">
                                                RELATIONSHIP WITH GOVERNMENT
                                            </h3>
                                        </div>
                                    </div>
                                    <div class="card-body">
                                        <div class="fv-row mb-10">
                                            <label for="dukunganPemerintahInput" class=" form-label">Required Support form the Government</label>
                                            <span class="ms-1" data-bs-toggle="tooltip" title="This column refers to information on support required from the government">
                                                <i class="ki-duotone ki-information-5 text-gray-500 fs-6">
                                                    <span class="path1"></span>
                                                    <span class="path2"></span>
                                                    <span class="path3"></span>
                                                </i>
                                            </span>
                                            @Html.TextAreaFor(m => m.SupportPemerintah, new
                                                {
                                                    @id = "dukunganPemerintahInput",
                                                    @class = "form-control form-control-solid",
                                                    placeholder = "Enter Required Support from the Government",
                                                    maxlength = 6000
                                                })
                                        </div>
                                        <div class="fv-row mb-10">
                                            <label for="keterlibatanLembagaInput" class=" form-label">Ministry / Agency Involvement</label><label style="padding-left: 10px;color: red;font-weight: bold;">Can select more than one</label>
                                            <span class="ms-1" data-bs-toggle="tooltip" title="This column refers to information on support from government agencies">
                                                <i class="ki-duotone ki-information-5 text-gray-500 fs-6">
                                                    <span class="path1"></span>
                                                    <span class="path2"></span>
                                                    <span class="path3"></span>
                                                </i>
                                            </span>
                                            @Html.DropDownListFor(x => x.KementrianLembagaIds,
                                                     new MultiSelectList(Enumerable.Empty<SelectListItem>()),
                                                     new
                                                     {
                                                         @id = "keterlibatanLembagaInput",
                                                         @class = "form-select form-select-solid form-select-lg fw-semibold",
                                                         aria_label = "keterlibatanLembagaInput",
                                                         data_control = "select2",
                                                         data_placeholder = "Choose Ministry / Agency Involvement",
                                                         data_allow_clear = "true"
                                                     })
                                        </div>

                                    </div>
                                </div>
                            </div>
                            <div class="flex-column-fluid">
                                <div class="card card-custom">
                                    <div class="card-header">
                                        <div class="row">
                                            <h3 class="card-title">
                                                ESCALATION POTENTIAL
                                            </h3>
                                        </div>
                                    </div>
                                    <div class="card-body">
                                        <div class="fv-row mb-10">
                                            <label for="potensialEskalasiInput" class=" form-label">Escalation Potential</label>
                                            <span class="ms-1" data-bs-toggle="tooltip" title="This column refers to information on potential escalation">
                                                <i class="ki-duotone ki-information-5 text-gray-500 fs-6">
                                                    <span class="path1"></span>
                                                    <span class="path2"></span>
                                                    <span class="path3"></span>
                                                </i>
                                            </span>
                                            @Html.TextAreaFor(m => m.PotensiEskalasi, new
                                                {
                                                    @id = "potensialEskalasiInput",
                                                    @class = "form-control form-control-solid",
                                                    placeholder = "Enter Escalation Potential",
                                                    maxlength = 6000
                                                })
                                        </div>
                                        <div class="fv-row mb-10">
                                            <label for="catatanTambahanInput" class=" form-label">Additional Note</label>
                                            <span class="ms-1" data-bs-toggle="tooltip" title="This column refers to information on additional notes">
                                                <i class="ki-duotone ki-information-5 text-gray-500 fs-6">
                                                    <span class="path1"></span>
                                                    <span class="path2"></span>
                                                    <span class="path3"></span>
                                                </i>
                                            </span>
                                            @Html.TextAreaFor(m => m.CatatanTambahan, new
                                                {
                                                    @id = "catatanTambahanInput",
                                                    @class = "form-control form-control-solid",
                                                    placeholder = "Enter Additional Note",
                                                    maxlength = 6000
                                                })
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-6">
                            <div class="flex-row-fluid">
                                <div class="card card-custom">
                                    <div class="card-header">
                                        <div class="row">
                                            <h3 class="card-title">
                                                PROJECT INFORMATION
                                            </h3>
                                        </div>
                                    </div>
                                    <div class="card-body">
                                        <div class="fv-row mb-10">
                                            <label for="negaraMitraInput" class="required form-label">Partner Region</label>
                                            <span class="ms-1" data-bs-toggle="tooltip" title="This column refers to the project area">
                                                <i class="ki-duotone ki-information-5 text-gray-500 fs-6">
                                                    <span class="path1"></span>
                                                    <span class="path2"></span>
                                                    <span class="path3"></span>
                                                </i>
                                            </span>
                                            @Html.DropDownListFor(x => x.NegaraMitraId,
                                                     new MultiSelectList(Enumerable.Empty<SelectListItem>()),
                                                     new
                                                     {
                                                         @id = "negaraMitraInput",
                                                         @class = "form-select form-select-solid form-select-lg fw-semibold",
                                                         aria_label = "negaraMitraInput",
                                                         data_control = "select2",
                                                         data_placeholder = "Choose Partner Region",
                                                         data_allow_clear = "true"
                                                     })
                                        </div>
                                        <div class="fv-row mb-10">
                                            <div id="addRepeaterLokasiProyek">
                                                <div class="form-group">
                                                    <label for="lokasiProyekInput" class="required form-label">Project Location</label>
                                                    <span class="ms-1" data-bs-toggle="tooltip" title="This column refers to the project location (can be more than one)">
                                                        <i class="ki-duotone ki-information-5 text-gray-500 fs-6">
                                                            <span class="path1"></span>
                                                            <span class="path2"></span>
                                                            <span class="path3"></span>
                                                        </i>
                                                    </span>
                                                    <div data-repeater-list="LokasiProyek">
                                                        <div data-repeater-item class="lokasiProyek mb-2">
                                                            <div class="fv-row form-group row">
                                                                <div class="col-md-10">
                                                                    @Html.TextBoxFor(x => x.AgreementLokasiProyek.LokasiProyek,
                                                                             new
                                                                             {
                                                                                 @id = "lokasiProyekInput",
                                                                                 @name = "LokasiProyek",
                                                                                 @class = "form-control form-control-solid",
                                                                                 maxlength = 100,
                                                                                 placeholder = "Project Location",
                                                                             })
                                                                </div>
                                                                <div class="col-md-2">
                                                                    <button class="border border-secondary btn btn-icon btn-flex btn-danger" data-repeater-delete type="button">
                                                                        <i class="la la-close"></i>
                                                                    </button>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="form-group row">
                                                    <div class="form-group mt-5">
                                                        <a href="javascript:;" data-repeater-create class="btn font-weight-bold btn-warning">
                                                            <i class="ki-duotone ki-plus fs-3"></i>
                                                            Add Project Location
                                                        </a>
                                                    </div>
                                                    <div class="col"></div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="fv-row mb-10">
                                            <label for="rangeTanggalTtdInput" class="required form-label">Signature Date</label>
                                            <span class="ms-1" data-bs-toggle="tooltip" title="This column refers to the agreement signing date">
                                                <i class="ki-duotone ki-information-5 text-gray-500 fs-6">
                                                    <span class="path1"></span>
                                                    <span class="path2"></span>
                                                    <span class="path3"></span>
                                                </i>
                                            </span>
                                            @Html.TextBoxFor(m => m.TanggalTtd, new
                                                {
                                                    @id = "rangeTanggalTtdInput",
                                                    @class = "form-control form-control-solid",
                                                    maxlength = 200,
                                                    placeholder = "Choose Signature Date"
                                                })
                                        </div>
                                        <div class="fv-row mb-10">
                                            <label for="rangeTanggalBerakhirInput" class="required form-label">End Date</label>
                                            <span class="ms-1" data-bs-toggle="tooltip" title="This column refers to the agreement end date">
                                                <i class="ki-duotone ki-information-5 text-gray-500 fs-6">
                                                    <span class="path1"></span>
                                                    <span class="path2"></span>
                                                    <span class="path3"></span>
                                                </i>
                                            </span>
                                            @Html.TextBoxFor(m => m.TanggalBerakhir, new
                                                {
                                                    @id = "rangeTanggalBerakhirInput",
                                                    @class = "form-control form-control-solid",
                                                    maxlength = 200,
                                                    placeholder = "Choose End Date"
                                                })
                                        </div>
                                        <div class="fv-row mb-10">
                                            <label for="statusBerlakuInput" class=" form-label">Valid Status</label>
                                            <span class="ms-1" data-bs-toggle="tooltip" title="This column refers to the validity status of the agreement">
                                                <i class="ki-duotone ki-information-5 text-gray-500 fs-6">
                                                    <span class="path1"></span>
                                                    <span class="path2"></span>
                                                    <span class="path3"></span>
                                                </i>
                                            </span>
                                            @Html.TextBoxFor(m => m.StatusBerlakuId, new
                                                {
                                                    @id = "statusBerlakuInput",
                                                    @class = "form-control form-control-solid",
